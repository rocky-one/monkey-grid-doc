{
    "include": ["@antv/gatsby-theme-antv", "src"],
    "compilerOptions": {
      "target": "es2019",
      "module": "commonjs",
      "lib": ["dom", "es2019"],
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "noEmit": true,
      "resolveJsonModule": true,
      "skipLibCheck": true
    }
  }
  